              valueFrom:
                secretKeyRef:
                  key: connect.clusters
                  name: RELEASE-NAME
            - name: LENSES_ALERT_CONSUMERS_LAG_THRESHOLD
              value: "50000"
            - name: LENSES_ALERT_PLUGINS
