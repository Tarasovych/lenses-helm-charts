              value: "1000"
            - name: LENSES_DEPLOYMENT_CONNECT_STATUS_INTERVAL
              value: 30 second
            - name: LENSES_DEPLOYMENT_CONNECT_ACTIONS_BUFFER_SIZE
              value: "1000"
