    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app: RELEASE-NAME
        chart: lenses-4.3.2
        component: lenses
        heritage: Helm
        lenses.io/app: RELEASE-NAME
        lenses.io/app.type: lenses-service
        release: RELEASE-NAME
      name: RELEASE-NAME
    spec:
      ports:
      - name: lenses
        port: 80
        targetPort: 3030
